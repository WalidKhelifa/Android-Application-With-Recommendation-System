     app:layout_constraintTop_toTopOf="parent"
     app:layout_constraintVertical_bias="0.761" />

 <TextView
     android:id="@+id/textView12"
     android:layout_width="130dp"
     android:layout_height="30dp"
     android:gravity="center"
